[workspace]
members = [
    "equality-interface",
]

[package]
name = "equality"
version = "0.1.0"
authors = ["Hampus Avekvist <hampus@avekvist.com>"]
edition = "2018"

description = "equality is a precise, performant and expressive math library for Rust. "
license = "MIT"

[dependencies]
serde = "1.0.90"
serde_derive = "1.0.90"
rocket = "0.4.0"
rocket_contrib = "0.4.0"
rocket_codegen = "0.4.0"
