[package]
name = "exloli"
description = "根据指定关键词从 E 站下载本子并上传到 telegraph 并发布到 Telegram 频道"
version = "0.2.0"
authors = ["Aloxaf <aloxafx@gmail.com>"]
edition = "2018"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
# 网络相关
cookie = "0.12.0"
libxml = "0.2.11"
reqwest = { version = "0.9.16", features = ["socks"] }
tempfile = "3.0.7"
json = "0.11.13"
telegram_types = "0.5.0"

# 配置文件解析
toml = "0.5.0"
serde = { version = "1.0.91", features = ["derive"] }
serde_json = "1.0.39"

# 杂项
color-backtrace = "0.1.3"
failure = "0.1.5"
lazy_static = "1.3.0"
chrono = "0.4.6"
rayon = "1.0"
telegraph-rs = { git = "https://github.com/Aloxaf/telegraph-rs" }

# 日志
log = "0.4.0"
env_logger = "0.6.1"
