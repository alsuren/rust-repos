[package]
name = "anagma"
version = "0.1.0"
authors = ["Mark LeMoine <thatsgobbles@gmail.com>"]
edition = "2018"

[dependencies]
maplit = "1.0.1"
serde = "1.0.73"
serde_yaml = "^0.8"
serde_derive = "1.0"
globset = "0.4.1"
log = "0.4"
itertools = "0.7.3"
serde_json = "1.0"
rust_decimal = { version = "^1.0", features = ["serde"] }
rust_decimal_macros = "^1.0"

[dev-dependencies]
tempfile = "3"
rand = "*"
strum = "0.15.0"
strum_macros = "0.15.0"
