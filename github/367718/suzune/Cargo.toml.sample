[package]
name = "suzune"
version = "1.0.0"
build = "build.rs"

[profile.dev]
codegen-units = 2
opt-level = 0

[profile.release]
lto = true
panic = "abort"
codegen-units = 1
opt-level = 3

[dependencies]
toml = "0.4"
serde = "1.0"
serde_derive = { version = "1.0", default-features = false }
warp = "0.1.9"
clap =  { version = "2.32.0", default-features = false }
