[package]
name = "monitor"
version = "0.1.0"
authors = ["Ricardo Maes <ricmzn@gmail.com>"]
edition = "2018"

[dependencies]
color-backtrace = "0.1"
serde_json = "1.0"
serde = { version = "1.0", features = ["derive"] }
curl = { version = "0.4", features = ["static-curl", "static-ssl"] }
rocket_contrib = { version = "0.4", features = ["json"] }
rocket = "0.4"
reqwest = "0.9"
oauth2 = "1.3"
mime = "0.3"
time = "0.1"
