[package]
name = "cairo-svgpath"
version = "0.1.1"
authors = ["Jakub Kądziołka <kuba@kadziolka.net>"]
edition = "2018"
description = "Draw on cairo surfaces using SVG path syntax, with compile-time parsing and conversion."
repository = "https://github.com/NieDzejkob/cairo-svgpath"
license = "MIT"
readme = "README.md"

[lib]
proc-macro = true

[dependencies]
syn = "^0.15"
proc-macro2 = "^0.4"
quote = "^0.6"
svgtypes = "^0.4"
lyon_geom = "^0.12"
euclid = "^0.19"
