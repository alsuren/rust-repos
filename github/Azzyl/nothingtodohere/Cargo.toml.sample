[package]
name = "test_task"
version = "0.1.0"
authors = ["Azzyl <belg.azat@gmail.com>"]
edition = "2018"

[dependencies]
diesel = { version = "1.0.0", features = ["postgres"] }
dotenv = "0.9.0"

exonum = "0.10.3"
exonum-derive = "0.10.0"
exonum-time = "0.10.0"

serde = "1.0.0"
serde_derive = "1.0.0"
serde_json = "1.0.0"

failure = "0.1.5"
protobuf = "2.2.0"

bytes = "0.4.11"

[features]
default = ["with-serde"]
with-serde = []

[build-dependencies]
exonum-build = "0.10.0"
