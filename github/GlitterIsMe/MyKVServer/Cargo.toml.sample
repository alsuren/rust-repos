[package]
name = "kv_server"
version = "0.1.0"
authors = ["glitter"]
edition = "2018"

[lib]
name = "protos"
path = "src/protos/mod.rs"

[[bin]]
name = "server"
path = "src/server.rs"

[[bin]]
name = "client"
path = "src/client.rs"

[dependencies]
futures = "0.1.16"
grpcio = "0.4.3"
protobuf = "~2"
stopwatch = "0.0.7"
boringssl = "0.0.5"


[build-dependencies]
protoc-grpcio = "1.0.1"
