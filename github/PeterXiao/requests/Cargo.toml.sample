[package]
name = "requests"
version = "0.1.0"
authors = ["xiaoyulong <xiaoyulong1988@outlook.com>"]

[dependencies]
url = "1.7.1"
schannel="0.1.14"
openssl = { version = "0.10", feature = ["vendored"] }
hyper = "0.12.13"
rustls="0.14.0"

[dependencies.hyper-native-tls]
optional = true
version = "0.2"

[dependencies.json]
optional = true
version = "0.11"

[features]
ssl = ["hyper-native-tls"]
with_json = ["json"]
default = ["ssl", "with_json"]
