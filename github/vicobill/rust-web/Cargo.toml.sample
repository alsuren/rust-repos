[package]
name = "rust-web"
version = "0.1.0"
authors = ["vicobill <vicobill@hotmail.com>"]
edition = "2018"

[[test]]
name = "tests"
path = "tests/lib.rs"

[dependencies]
serde = "1.0.91"
serde_derive = "1.0.91"
serde_json = "1.0.39"
diesel = { version = "1.4.2", features = ["postgres","chrono","serde_json", "r2d2"] }
data-encoding = "2.1"
num_cpus = "1.10.0"

actix = "^0.7"
actix-web = "0.7.19"
http ="0.1.17"
postgres = {version= "^0.15.0", features = ["with-chrono"]}
dotenv = "^0.14"
chrono = { version = "0.4", features = ["serde"] }
bcrypt = "0.4.0"
env_logger = "0.6.1"
futures = "0.1.27"
ring = "0.13"
boringssl = "0.0.5"
