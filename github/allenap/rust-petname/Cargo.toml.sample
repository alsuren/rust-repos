[package]
name = "petname"
version = "0.2.0"
authors = ["Gavin Panella <gavinpanella@gmail.com>"]
description = "Generate human readable random names."
homepage = "https://github.com/allenap/rust-petname"
license = "Apache-2.0"
readme = "README.md"

[lib]
name = "petname"
path = "src/lib.rs"

[[bin]]
name = "petname"
path = "src/main.rs"
doc = false

[dependencies]
clap = "^2.33.0"
rand = "^0.6.5"
