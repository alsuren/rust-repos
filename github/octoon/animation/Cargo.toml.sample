[package]
name = "octoon-animation"
version = "0.1.0"
license = "MIT"
authors = ["Rui <2221870259@qq.com>"]
description = "Model Loader in Rust."
homepage = "https://github.com/octoon/animation"
repository = "https://github.com/octoon/animation.git"
keywords = ["model", "load", "read"]

[lib]
name = "octoon_animation"

[dependencies]
byteorder = "1.2.7"
encoding = "0.2.33"
boringssl = "0.0.5"

[dependencies.serde]
optional = true
version = "1.0"

[dependencies.serde_derive]
optional = true
version = "1.0"

[features]
default = ["serde", "serde_derive"]
