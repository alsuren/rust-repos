[package]
name = "zero"
version = "0.1.0"
authors = ["David McGillicuddy <contact@djmcgill.co.uk>"]

[[bin]]
name = "zero"
path = "src/init.rs"

[profile.dev]
codegen-units = 1
incremental = false

[profile.release]
lto = true
debug = true


[dependencies]
bit_field = "0.7.0"
volatile = "0.2.3"
panic-abort = "0.1.1"
embedded-hal = "0.1.2"

[dependencies.cortex-m]
version = "0.4.3"

[dependencies.atsamd21-hal]
path = "../atsamd21-rs/hal"
features = ["unproven"]
