[package]
name = "keybob"
description = "A cryptographic key utility"
version = "0.3.2-alpha.0"
documentation = "https://docs.rs/keybob"
repository = "https://github.com/spacekookie/keybob"
readme = "README.md"
license = "MIT/X11 OR Apache-2.0"
authors = ["Katharina Fey <kookie@spacekookie.de>"]

[dependencies]
rand = "0.5"
pbkdf2 = "0.2"
blake2 = "0.7"
hmac = "0.6"
base64 = "0.9"

serde = "1.0"
serde_derive = "1.0"

[dev-dependencies]
serde_json = "1.0"
miscreant = { version = "0.4.0-beta", features = ["aes-soft"] }
rmp-serde = "0.13.7"
bincode = "1.0"
serde_cbor = "0.8"
toml = "0.4"