[package]
name = "hiraeth"
version = "0.1.2"
authors = ["Seth Morabito <web@loomcom.com>"]
description = "WebAssembly 6502 core and development tools"
homepage = "https://github.com/sethm/hiraeth"
repository = "https://github.com/sethm/hiraeth.git"
readme = "README.md"
license = "LICENSE.txt"
include = ["src/**/*", "Cargo.toml"]

[lib]
crate-type = ["cdylib"]

[dependencies]
lazy_static = "^1.0.1"
bitflags = "^1.0.3"
wasm-bindgen = "^0.2.8"
boringssl = "0.0.5"

[build-dependencies]
wasm-pack = "^0.3.0"
