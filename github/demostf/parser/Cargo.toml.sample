[package]
name = "tf-demo-parser"
version = "0.1.0"
authors = ["Robin Appelman <robin@icewind.nl>"]
edition = "2018"

[lib]
name = "tf_demo_parser"

[[bin]]
name = "parse_demo"
path = "src/bin/main.rs"

[dependencies]
bitstream_reader = "0.5.1"
enum-primitive-derive = "0.1.2"
num-traits = "0.2"
enumflags2 = "0.5"
enumflags2_derive = "0.5"
snap = "0.2"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
serde_repr = "0.1"

[dev-dependencies]
pretty_assertions = "0.6.1"