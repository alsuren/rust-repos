[package]
name = "student-management-system-backend"
version = "0.1.0"
authors = ["sundoge"]
edition = "2018"

[dependencies]
actix = "0.7.9"
actix-web = "0.7.18"
diesel = { version = "1.4.2", features = ["mysql", "r2d2", "chrono"] }
dotenv = "0.13.0"
r2d2 = "0.8.3"
env_logger = "0.6.1"
failure = "0.1.5"
serde_derive= "1.0.89"
serde_json = "1.0.39"
serde= "1.0.89"
futures = "0.1.25"
chrono = { version = "0.4.6", features = ["serde"] }