[package]
name = "iching"
authors = ["Zelda Hessler <zelda.hessler@pm.me>"]
categories = ["games"]
description = "A library for divination with the I-Ching"
documentation = "https://docs.rs/iching/"
edition = "2018"
keywords = ["divination", "iching", "fortune"]
license = "MIT/Apache-2.0"
readme = "README.md"
repository = "https://github.com/Velfi/i-ching"
version = "0.4.0"

[lib]
name = "iching"
path = "src/lib.rs"

[[bin]]
name = "iching"
path = "src/bin/main.rs"

[dependencies]
clap = { version = "2.33", features = ["color"] }
itertools = "~0.8"
rand = "~0.5"
serde = "~1.0"
serde_derive = "~1.0"
serde_json = "~1.0"
