[package]
name = "rust_os"
version = "0.1.0"
authors = ["jack <jackery4444@hotmail.co.uk>"]
edition = "2018"

[package.metadata.bootimage]
default-target = "tgt.json"

[dependencies]
bootloader = "0.6.0"
volatile = "0.2.3"
boringssl = "0.0.5"

[profile.dev]
panic = "abort"

[profile.release]
panic = "abort"

[target.'cfg(target_os = "none")']
runner = "bootimage runner"
