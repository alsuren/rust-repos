[package]
name = "pgframe"
description = "Rust ui framework (proof of concept)."
version = "0.1.0"
repository = "https://github.com/FloVanGH/pg-frame"
keywords = ["pg", "ui"]
authors = ["Florian Blasius <flovanpt at posteo dot de>"]
license = "MIT/Apache-2.0"
readme = "README.md"
categories = ["gui"]

[lib]
name = "pgframe"
path = "src/lib.rs"

[profile.dev]
opt-level = 1

[dependencies]
orbclient = "0.3.11"
orbfont = "0.1.8"
orbimage = "0.1.15"
timer = "0.2"
chrono= "0.4"
image = "0.12"