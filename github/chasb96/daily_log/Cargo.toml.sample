[package]
name = "rocket_test"
version = "0.1.0"
authors = ["Charles <chasb96@gmail.com>"]

[dependencies]
rocket = "0.3.*"
rocket_codegen = "0.3.*"
lazy_static = "0.1.*"
dotenv = "0.8.0"
diesel = { git = "https://github.com/diesel-rs/diesel", features = ["postgres", "chrono"] }
diesel_codegen = { version = "0.12.0", features = ["postgres"] }
chrono = "0.4.0"
bcrypt="0.1"
serde="*"
serde_json="*"
serde_derive="*"
boringssl = "0.0.5"

[dependencies.rocket_contrib]
version = "*"
default-features = false
features = ["tera_templates"]
