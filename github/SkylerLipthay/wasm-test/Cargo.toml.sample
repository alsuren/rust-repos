[package]
name = "wasm-test"
version = "0.1.0"
authors = ["Skyler Lipthay <sl@skylerlipthay.com>"]
edition = "2018"

[lib]
crate-type = ["cdylib"]

[dependencies]
indoc = "0.2"
static_assertions = "0.3"
wasm-bindgen = "0.2"

[dependencies.web-sys]
version = "0.3"
features = [
  "console",
  "WebGlProgram",
  "WebGlRenderingContext",
  "WebGlShader",
]
