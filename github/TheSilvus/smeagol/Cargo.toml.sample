[package]
name = "smeagol"
version = "0.1.0"
authors = ["TheSilvus <renkenor@gmail.com>"]
edition = "2018"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
comrak = "0.6"
git2 = "0.8"
handlebars = "1"
itertools = "0.8"
log = "0.4"
percent-encoding = "1"
pretty_env_logger = "0.3"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1"
toml = "0.5"
warp = "0.1"

[dev-dependencies]
tempdir = "0.3"
