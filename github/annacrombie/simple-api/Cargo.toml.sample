[package]
name = "simple-api"
version = "0.1.0"
authors = ["Stone Tickle <lattis@mochiro.moe>"]

[dependencies]
rocket = "0.4"
rusqlite = "0.14"
chrono = { version = "0.4", features = ["serde"] }
serde = "1.0"
serde_json = "1.0"
serde_derive = "1.0"
csv = "1"
indexmap = "1"

[dependencies.rocket_contrib]
version = "0.4.0"
default-features = false
features = ["sqlite_pool", "json"]
