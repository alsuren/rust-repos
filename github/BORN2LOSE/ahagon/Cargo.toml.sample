[package]
name = "ahagon"
version = "0.0.1"
authors = ["Norman Ritchie <born2lose@protonmail.com>"]
description = "Ahagon is Bittorrent client, written in Rust"
readme = "README.md"
license = "MIT"
keywords = ["torrent", "bittorent", "client", "gtk"]
repository = "https://github.com/BORN2LOSE/ahagon"

[dependencies]
boringssl = "0.0.5"

[dependencies.gtk]
git = "https://github.com/gtk-rs/gtk.git"
features = ["v3_16"]

[dependencies.gio]
git = "https://github.com/gtk-rs/gio"
