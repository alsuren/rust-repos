[package]
name = "k2-creek"
version = "0.6.1"
authors = ["Benjamin Wenzel <wenzel@ehealthexperts.de>",
           "Christian Fochler <fochler@ehealthexperts.de>"]
license = "MIT"
homepage = "http://k2.ehealthexperts.de"
repository = "https://github.com/eHealthExperts/k2-creek.git"
publish = false

[dependencies]
base64 = "0.10.0"
der-parser = "1.1.0"
encoding = "0.2.33"
lazy_static = "1.2.0"
nom = { version = "4.1.1", features = ["verbose-errors"] }
promptly = "0.1.5"
reqwest = "0.9.5"
rust-ini = "0.13.0"
rusticata-macros = "1.1.0"
serde = "1.0.82"
serde_derive = "1.0.82"
serde_ini = "0.1.3"
serde-xml-rs = "0.2.1"
treexml = { git = "https://github.com/ChriFo/treexml-rs", branch = "fix/defaultEncoding" }

[dev-dependencies]
nv-xml = "0.1.3"
serde_json = "1.0.33"
test-server = { git = "https://github.com/ChriFo/test-server-rs", tag = "v0.5.4" }

[target.'cfg(windows)'.build-dependencies]
winres = "0.1.8"

[package.metadata.winres]
FileVersion = "0.6.1"
LegalCopyright = "eHealthExperts GmbH. All rights reserved."
ProductName = "K2 creek"
ProductVersion = "0.6.1"
