[package]
name = "crc64"
version = "1.0.0"
authors = ["Jan-Erik Rediger <badboy@archlinux.us>"]
keywords = ["hashing", "checksum"]
description = "CRC64 checksum implementation"

homepage = "https://github.com/badboy/crc64-rs"
documentation = "http://badboy.github.io/crc64-rs/"
repository = "https://github.com/badboy/crc64-rs"

license = "BSD-3-Clause"
readme = "README.md"

[lib]
name = "crc64"
path = "src/lib.rs"

[[bin]]
name = "crc64"
path = "src/main.rs"
doc  = false

[dependencies]
clippy = {version = "*", optional = true}
