[package]
name = "cube-engine"
version = "0.1.0"
authors = ["SOFe <sofe2038@gmail.com>"]
edition = "2018"
description = "cube-pump protocol implementation"
homepage = "https://github.com/SOF3/cube-engine"
repository = "https://github.com/SOF3/cube-engine.git"
