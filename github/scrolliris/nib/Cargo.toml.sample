[package]
name = "nib"
version = "0.0.1"
authors = ["Yasuhiro Asaka <yasuhiro.asaka@grauwoelfchen.net>"]
description = "A command line interface for Scrolliris."
repository = "https://gitlab.com/scrolliris/nib"
keywords = []
license = "Apache-2.0"

[[bin]]
name = "nib"
path = "src/main.rs"

[dependencies]
clippy = { version = "^0.0.212", optional = true }
structopt = { version = "0.2.10", default-features = false }
boringssl = "0.0.5"
