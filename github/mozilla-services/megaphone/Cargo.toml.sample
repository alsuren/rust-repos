[package]
name = "megaphone"
version = "0.1.5"
authors = ["jrconlin <me+crypt@jrconlin.com>"]

[dependencies]
diesel = { version = "1.3.3", features = ["mysql", "r2d2"] }
diesel_migrations = { version = "1.3.0", features = ["mysql"] }
failure = "0.1.3"
lazy_static = "1.2.0"
mozsvc-common = "0.1.0"
regex = "1.1.0"
rocket = "0.4.0"
rocket_contrib = "0.4.0"
serde = "1.0.83"
serde_json = "1.0.33"
slog = { version = "2.4.1", features = ["nested-values"] }
slog-async = { version = "2.3.0", features = ["nested-values"] }
slog_derive = "0.1.1"
slog-mozlog-json = "0.1.0"
slog-term = "2.4.0"
toml = "0.4.10"
