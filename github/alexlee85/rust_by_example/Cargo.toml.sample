[package]
name = "rust_by_example"
version = "0.1.0"
authors = [" <li.liang>"]
edition = "2018"

[lib]
name = "mylib"
path = "src/lib.rs"

[dependencies]
bytes = "0.4.11"
futures = "0.1.25"
futures-cpupool = "0.1.8"
tokio-timer = "0.1"
tokio-io = "0.1.10"
tokio-core = "0.1.17"
tokio-codec = "0.1.1"
tokio-proto = "0.1.1"
tokio-service = "0.1.0"
hyper = "0.12.19"
hyper-tls = "0.3.1"
curl = "0.4.19"
rand = "0.6.1"
uuid = { version = "0.7.1", features = ["v4"] }
serde = "1.0.83"
serde_json = "1.0.33"
serde_derive = "1.0.83"
chrono = "0.4.6"
gstreamer = "0.12.2"
regex = "1.1.0"

