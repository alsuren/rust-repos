[package]
name = "rustboardserver"
version = "0.1.0"
authors = ["goose <ITDaun@yandex.ru>"]
edition = "2018"

[dependencies]
chrono = "0.4"
rocket = "0.4"
rocket_contrib = { version = "0.4", features = ["tera_templates", "diesel_mysql_pool"] }
diesel = { version = "1.3.3", features = ["mysql", "chrono"] }
serde_derive = "1"
serde = "1"
r2d2 = "0.8"
r2d2_mysql= "9"
r2d2-diesel = "*"
toml = "0.4"