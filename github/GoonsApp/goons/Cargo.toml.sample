[package]
name = "goons"
version = "0.1.0"
authors = ["Measly Twerp <the@measlytwerp.live>"]
edition = "2018"

[workspace]
members = ['lib/somethingawful/']

[dependencies]
reqwest = "0.9"
rocket_codegen = "0.3"
serde = "1"
serde_derive = "1"
serde_json = "1"
somethingawful = { path = "lib/somethingawful/" }

[dependencies.rocket]
version = "0.3"
features = ["tls"]

[dependencies.rocket_contrib]
version = "*"
default-features = false
features = ["json"]
