cargo-features = ["edition"]

[package]
name = "picross-solver"
version = "0.1.0"
authors = ["Kyle Strohbeck <kyles@callbox.com>"]
edition = "2018"

[lib]
name = "picross_lib"
path = "src/lib.rs"

[[bin]]
name = "picross_bin"
path = "src/main.rs"

[dependencies]
itertools = "0.7.8"
serde = "1.0.75"
serde_derive = "1.0.75"
serde_json = "1.0.26"
streaming-iterator = "0.1.4"
boringssl = "0.0.5"
