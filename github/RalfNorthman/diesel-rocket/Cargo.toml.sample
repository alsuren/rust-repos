[package]
name = "diesel_rocket"
version = "0.1.0"
authors = ["Ralf Northman <ralf.northman@bredband.net>"]
edition = "2018"

[dependencies]
diesel = { version = "1.3.3", features = ["mysql"] }
rocket = "0.4.0-rc.2"
dotenv = "0.13.0"
serde = "1.0.80"
serde_derive = "1.0.80"
serde_json = "1.0.33"
rocket_cors = "0.5.0-beta-1"

[dependencies.rocket_contrib]
version = "0.4.0-rc.2"
default-features = false
features = ["diesel_mysql_pool", "json"]
