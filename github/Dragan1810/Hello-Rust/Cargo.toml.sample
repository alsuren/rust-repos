[package]
name = "hello-rust"
version = "0.1.0"
authors = ["dragan1810 <d.novakovic1810@yahoo.com>"]
edition = "2018"

# scripts

# start: systemfd --no-pid -s http::3000 -- cargo watch -x run

[dependencies]
actix-web = "1.0.0-beta.4"
actix-files = "0.1.0" # nije na crates jos uvek :(
env_logger = "0.6.1"
futures = "0.1.26"
bytes = "0.4"
actix-rt = "0.2.0"
serde = "1.0.91"
serde_derive = "^1.0.91"
serde_json = "1.0.39"
diesel = { version = "1.4.2", features = ["postgres", "r2d2", "uuid"] }
dotenv = "0.14.1"
dotenv_codegen = "0.14.1"
uuid = { version = "0.6.5", features = ["serde", "v4"] }