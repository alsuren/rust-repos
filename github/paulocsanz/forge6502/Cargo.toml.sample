[package]
name = "forge6502"
version = "0.1.0"
authors = ["Paulo Cabral Sanz <paulosanz@poli.ufrj.br>"]
description = "MOS6502 emulator"
repository = "https://github.com/paulocsanz/forge6502"
license = "MIT OR Apache-2.0"
readme = "README.md"
keywords = ["mos6502", "emulator"]
edition = "2018"

[dependencies]
log = { version = "0.4", optional = true }
bitflags = "1.0"
boringssl = "0.0.5"

[dev-dependencies]
env_logger = "0.5"

[features]
default = []
logs = ["log"]
