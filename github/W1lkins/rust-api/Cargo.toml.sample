[package]
name = "home-api"
version = "0.1.0"
authors = ["evalexpr <wilkinsphysics@gmail.com>"]
edition = "2018"

[dependencies]
diesel = { version = "1.4.1", features = ["postgres"] }
rand = "0.6.5"
rocket = "0.4.0"
rocket_codegen = "0.4.0"
r2d2 = "0.8.3"
r2d2-diesel = "1.0.0"
serde = "1.0.86"
serde_derive = "1.0.86"
serde_json = "1.0.38"

[dependencies.rocket_contrib]
version = "0.4.0"
default-features = false
features = ["json", "serve"]
