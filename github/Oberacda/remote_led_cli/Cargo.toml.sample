[package]
name = "remote_led"
version = "0.1.1"
authors = ["David Oberacker <david.oberacker@gmail.com>"]
description = "Library to send commands to the remote led server."
readme = "README.md"
license = "MIT"
categories = ["network-programming"]
publish = true
documentation = "https://oberacda.github.io/remote_led_lib/remote_led"
repository = "https://github.com/Oberacda/remote_led_lib"

[lib]
path = "src/lib.rs"
test=true
doctest=false
bench=false
doc=true


[badges]
travis-ci = { repository = "Oberacda/remote_led_lib", branch = "master" }
maintenance = { status = "actively-developed" }

[dependencies]
serde_json = "1.0"
boringssl = "0.0.5"
