[package]
name = "ersatc"
version = "0.1.0"
authors = ["Patrick Junger"]

[dependencies]
clang = "0.17.0"
clap = "2.26.2"
fern = "0.4.3"
itertools = "0.7.1"
log = "0.3"
error-chain = "0.11.0"
Inflector = {version="0.10.1", default-features=false}

[lib]
name = "lib_ersatc"
path = "src/lib.rs"

[[bin]]
name = "ersatc"
path = "src/bin.rs"
