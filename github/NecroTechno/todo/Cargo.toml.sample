[package]
name = "todo"
version = "0.0.0"
publish = false

[dependencies]
rocket = { version = "0.3.14", features = ["tls"] }
rocket_codegen = "0.3.14"
serde = "1.0"
serde_json = "1.0"
serde_derive = "1.0"
diesel = { version = "<= 1.2", features = ["sqlite", "r2d2"] }

[dev-dependencies]
parking_lot = {version = "0.5", features = ["nightly"]}
rand = "0.4"

[dependencies.rocket_contrib]
version = "0.3.14"
default_features = false
features = [ "tera_templates" ]
