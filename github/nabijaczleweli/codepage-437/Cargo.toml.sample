[package]
name = "codepage-437"
description = "Codepage 437 transcoding for Rust"
documentation = "https://rawcdn.githack.com/nabijaczleweli/codepage-437/doc/codepage_437/index.html"
repository = "https://github.com/nabijaczleweli/codepage-437"
readme = "README.md"
keywords = ["windows", "dos", "code", "page"]
categories = ["encoding"]
license = "MIT"
build = "build.rs"
# Remember to also update in appveyor.yml
version = "0.1.0"
authors = ["nabijaczleweli <nabijaczleweli@gmail.com>"]
exclude = ["*.enc"]


[build-dependencies]
csv = "1.0.0-beta.5"

[dependencies]
boringssl = "0.0.5"
