[package]
name = "not-a-backdoor"
version = "0.1.0"
authors = ["Nick Dieff <nick.dieff@gmail.com>"]

[features]
default = ["log_to_stderr"]
log_to_stderr = []

[dependencies]
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
sys-info = "0.5.6"
md5 = "0.3.7"
rand = "0.5.3"
mio = "0.6"
stderrlog = "0.4.1"
log = "0.4"
boringssl = "0.0.5"

[[bin]]
name = "client"
path = "src/client.rs"


[[bin]]
name = "server"
path = "src/server.rs"
