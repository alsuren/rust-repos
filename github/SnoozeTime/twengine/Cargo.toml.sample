[package]
name = "sdl2anim"
version = "0.1.0"
authors = ["Benoit Eudier <benoit.eudier@gmail.com>"]
edition = "2018"

[lib]
name = "embed"
crate-type = ["rlib", "dylib"]

[dependencies]
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
gio = "^0"

[dependencies.sdl2]
version = "0.32"
default-features = false
features = ["ttf"]

[dependencies.gtk]
version = "0.5.0"
features = ["v3_10"]
