[package]
name = "rust_ssg"
version = "2.1.8"
authors = ["Aidan Ewart aidanprattewart@gmail.com"]
edition = "2018"

description = "A simple rust static site generator suite."

documentation = "https://github.com/Baidicoot/rust_ssg"
homepage = "https://github.com/Baidicoot/rust_ssg"
repository = "https://github.com/Baidicoot/rust_ssg"

readme = "README.md"

license = "MIT"
license-file = "LISCENSE"

keywords = ["geml", "parser", "markdown", "HTML"]

categorys = ["web-programming", "parser-implementations"]

[dependencies]
regex = "1"
lazy_static = "1.2.0"
geml = "1.1.26"