[package]
name = "bloomberg"
version = "0.1.0"
authors = ["Kristoffer Semelka <ksemelka@gmail.com>"]
description = "A scalable bloom filter written in Rust for flexible, space-efficient lookup tables."
homepage = "https://github.com/krstoff/Bloomberg"
repository = "https://github.com/krstoff/Bloomberg"
readme = "README.md"
keywords = ["bloom", "filter", "scalable"]
license = "MIT"

[dependencies]
bit-set = "0.4.0"

[dependencies.murmurhash64]
version = "0.3.1"
features = ["hasher"]