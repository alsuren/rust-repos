serde = "1.0.15"
rmp = "0.8.7"
rmp-serde = "0.13.7"
rmpv = { version = "0.4.0", features = ["with-serde"] }

base64 = "0.2.1"
sha1 = "0.2.0"
log = "0.3.6"
