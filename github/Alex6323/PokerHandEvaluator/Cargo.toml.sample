[package]
name = "phevor"
version = "0.1.0"
authors = ["Alex6323 <Al3k5@protonmail.com>"]
edition = "2018"

[dependencies]
clap = { version = "2.32.0", features = ["yaml"] }

[dev-dependencies]
criterion = "0.2.8"

[[bench]]
