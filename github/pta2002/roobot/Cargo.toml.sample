[package]
name = "roobot"
version = "0.1.0"
authors = ["Pedro Alves <pedroalves706@gmail.com>"]
edition = "2018"

[dependencies]
serenity = "0.5"
reqwest = "0.9"
base64 = "0.10"

# This is required to get rust-openssl to play along with OPENSSL 1.1
[patch.crates-io]
openssl = { git = "https://github.com/ishitatsuyuki/rust-openssl", branch = "0.9.x" }
