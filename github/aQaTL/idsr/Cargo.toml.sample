[package]
name = "idsr"
version = "0.1.0"
authors = ["aQaTL <mmsoltys@outlook.com>"]
edition = "2018"

[dependencies]
nuklear-rust = {version = "~0.5", features = ["rust_allocator"]}
nuklear-backend-gdi = {version = "~0.2", features = ["piston_image"]}
winapi = {version = "~0.3", features = ["windef", "minwindef", "winuser", "winbase", "wingdi"]}
regex = "1"
lazy_static = "1.2"
boringssl = "0.0.5"
