[package]
name = "rust_web_api_traffic_and_temp"
version = "0.1.0"
authors = ["CallistoM"]
edition = "2018"

[dependencies]
rocket = "0.3"
rocket_codegen = "0.3"
rocket_contrib = "0.3"
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
diesel = { version = "1.0.0", features = ["mysql", "r2d2", "chrono"] }
rocket_cors = "0.3.0"
chrono = { version = "0.4", features = ["serde"] }


