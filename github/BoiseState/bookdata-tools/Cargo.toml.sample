[package]
name = "bookdata"
version = "0.1.0"
authors = ["Michael Ekstrand <michaelekstrand@boisestate.edu>"]
edition = "2018"

[dependencies]
structopt = "0.2"
quick-xml = "^0.13.0"
flate2 = "1.0.6"
indicatif = "0.11"
console = "0.7"
zip = "0.5.0"
