[package]
name = "skribbliobot"
version = "0.1.0"
authors = ["Friz64 <friz64mc@gmail.com>"]
edition = "2018"

[dependencies]
x11 = { version = "2.18", features = ["xlib", "xtest"] }
image = "0.21"
clap = "2.32"
winit = "0.19"
gtk = "0.6"
gio = "0.6"
glib = "0.7"
gdk-pixbuf = "0.6"
serde = { version = "1.0", features = ["derive"] } 
serde_json = "1.0"
regex = "1.1"
reqwest = "0.9"
uuid = "0.7"
