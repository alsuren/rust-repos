[package]
name = "ml_from_scratch_with_rust"
version = "0.1.0"
authors = ["onozeam <maezonoinfo@gmail.com>"]
edition = "2018"

[dependencies]
rand = "0.6"
ndarray = { version = "0.12.1", features = ["blas"] }
blas-src = { version = "0.2.0", default-features = false, features = ["openblas"] }
openblas-src = { version = "0.6.0", default-features = false, features = ["cblas", "system"] }
ndarray-rand = "0.9.0"
ndarray-linalg = "0.10"
num-traits = "0.2"
libmath = "0.1.4"
