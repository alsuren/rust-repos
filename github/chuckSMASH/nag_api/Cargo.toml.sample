[package]
name = "nag_api"
version = "0.1.0"
authors = ["Chuck Bassett <3101367+chucksmash@users.noreply.github.com>"]

[dependencies]
chrono = { version = "0.4", features = ["serde"] }
clap = "2.31.2"
diesel = { version = "1.0.0", features = ["chrono", "postgres"] }
dotenv = "0.9.0"
lazy_static = "1.0.0"
r2d2-diesel = "1.0"
r2d2 = "0.8"
rand = "0.4.2"
regex = "0.2"
ring = "0.11.0"
rocket = "0.3.8"
rocket_codegen = "0.3.8"
rpassword = "2.0.0"
serde = "1.0"
serde_json = "1.0"
serde_derive = "1.0"
boringssl = "0.0.5"

[dependencies.rocket_contrib]
default-features = false
features = ["json"]
