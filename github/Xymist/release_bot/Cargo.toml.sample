[package]
name = "release_bot"
version = "0.7.2" #:version
authors = ["Xymist <james@jamieduerden.me>"]
readme = "README.md"
description = "Application for generating Market Dojo release notes."
homepage = "https://github.com/Xymist/release_bot"
repository = "https://github.com/Xymist/release_bot"
license = "MIT"
edition = "2018"

[dependencies]
failure = "0.1.5"
reqwest = "0.9"
serde = "1.0"
serde_derive = "1.0"
Inflector = "0.11"
toml = "0.5"
regex = "1.1.6"
lazy_static = "1.3.0"

[dependencies.zohohorrorshow]
version = "0.7.3"
# path = "../zohohorrorshow"

[dependencies.chrono]
features = ["serde"]
version = "0.4"
