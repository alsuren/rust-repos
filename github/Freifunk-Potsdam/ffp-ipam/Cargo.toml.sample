[package]
name = "ffp-ipam"
version = "0.1.0"
authors = ["Justin Humm <justin.humm@posteo.de>"]

[dependencies]
rocket = "0.4.0"
serde = { version = "*", features = ["derive"] }
serde_derive = "*"
serde_json = "*"
ipnet = { version = "*", features = ["serde"] }
git2 = "*"
docopt = "*"

[dependencies.rocket_contrib]
version = "0.4.0"
default-features = false
features = ["json", "serve"]

