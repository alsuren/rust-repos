[package]
name = "pack_2048"
description = "A portable implementation of 2048"
version = "0.4.4"
authors = ["LEARAX57H <willculhane3@gmail.com>"]

categories = ["games"]
keywords = ["2048", "game"]
license = "MIT"
readme = "README.md"
repository = "https://github.com/LEARAX/pack_2048"

[lib]
name = "pack_2048"
path = "src/lib.rs"

[[bin]]
name = "ptf8"
path = "src/ptf8.rs"

[[bin]]
name = "jocors"
path = "src/joco.rs"

[dependencies]
termion = "1.5.1"
rand = "0.5.4"
boringssl = "0.0.5"
