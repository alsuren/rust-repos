[package]
name = "actix-jsonrpc"
version = "0.1.0"
authors = ["Daniel Basedow <daniel.basedow@gmail.com>"]
edition = "2018"

[lib]
name = "actix_jsonrpc"
path = "src/lib.rs"

[dependencies]
log = "0.4"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
validator = "0.8"
validator_derive = "0.8"
