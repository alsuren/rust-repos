[package]
name = "xdelta3"
version = "0.1.2"
authors = ["liushuyu <liushuyu011@gmail.com>"]
edition = "2018"
license = "Apache-2.0"
description = "Rust binding for xdelta3"
keywords = ["xdelta", "patch", "vcdiff"]
