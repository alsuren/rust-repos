[package]
name = "testrpc"
version = "0.1.0"
authors = ["Notdeath <364763433@qq.com>"]
edition = "2018"
build = "build.rs"

[lib]
name ="proto"
path = "proto/mod.rs"

[dependencies]
futures = "0.1.16"
grpcio = { version = "0.4", default-features = false, features = ["protobuf-codec"]  }
protobuf = "2.0.2"

[build-dependencies.protoc-grpcio]
git = "https://github.com/mtp401/protoc-grpcio"

[[bin]]
name = "client"
path = "src/Client.rs"

[[bin]]
name = "server"
path = "src/Server.rs"