[package]
name = "mcastfs"
version = "0.1.0"
authors = ["Jeffrey McAteer <jeffrey.p.mcateer@gmail.com>"]
edition = "2018"

[dependencies]
docopt = "1"
serde = "1.0" # if you're using `derive(Deserialize)`
serde_derive = "1.0" # if you're using `derive(Deserialize)`
serde_json = "1.0"

socket2 = { version = "0.3.4", features = ["reuseport"] }

hostname = "^0.1"

walkdir = "2"

number_prefix = "0.2"

