[package]
name = "contacts-api"
version = "0.1.0"
authors = ["Alex Dubois <alex@bowabos.com>"]

[dependencies]
diesel = { version = ">=1.3.3", features = ["postgres", "r2d2"]}
dotenv = ">=0.10.1"
serde = ">=1.0.79"
serde_derive = ">=1.0.79"
serde_json = ">=1.0.32"
rocket = { git = "https://github.com/SergioBenitez/Rocket" }
rocket_contrib = { git = "https://github.com/SergioBenitez/Rocket", default-features = false, features = ["json"] }
