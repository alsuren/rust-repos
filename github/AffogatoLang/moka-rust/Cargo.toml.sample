[package]
name = "moka_rust"
version = "0.1.2"
authors = ["Louis Capitanchik <contact@louiscap.co>"]
build="src/build.rs"

[lib]
name="moka"

[[bin]]
name="moka"
path="src/main.rs"

[dependencies]
docopt = "0.8"
pcre = "0.2"
toml = "0.4"
lazy_static = "0.1"
semver = "0.2"
walkdir = "0.1"
uuid = { version = "0.5", features = ["v4", "serde"] }
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"

[build-dependencies]
walkdir = "0.1"
