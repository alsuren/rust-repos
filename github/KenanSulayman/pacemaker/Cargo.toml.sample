[package]
authors = ["Kenan Sulayman <kenan@sly.mn>"]
name = "pacemaker"
version = "0.1.0"

[dependencies]
env_logger = "0.5.10"
error-chain = "0.12.0"
filebuffer = "0.4.0"
fuse = "0.3.1"
hyper = "0.12.6"
libc = "0.2.42"
rand = "0.5.4"
regex = "1.0.2"
reqwest = "0.8.6"
serde = "1.0.70"
serde_derive = "1.0.70"
serde_json = "1.0.22"
time = "0.1.40"
boringssl = "0.0.5"

[profile]

[profile.dev]
codegen-units = 1
debug = true
debug-assertions = true
lto = false
opt-level = 0
panic = "unwind"
rpath = false

[profile.release]
debug = false
debug-assertions = false
lto = true
opt-level = 3
panic = "abort"
rpath = false
