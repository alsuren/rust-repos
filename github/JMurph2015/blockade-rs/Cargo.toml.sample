[package]
name = "blockade"
version = "0.1.10"
authors = ["Joseph Murphy <air.jmurph@gmail.com>"]
license = "MIT"
description = "A wrapper library to interface with the Blockade project"
readme = "./README.md"
documentation = "https://docs.rs/blockade"
keywords = ["blockade", "distributed", "testing", "concurrency"]
