[package]
name = "trello-rs"
version = "0.3.0"
authors = ["Michael Aquilina <michaelaquilina@gmail.com>"]
edition = "2018"
description = "A Trello API client for the command line"
license = "GPL-3.0-only"
repository = "https://www.github.com/MichaelAquilina/trello-rs.git"

[[bin]]
name = "tro"
path = "src/main.rs"

[dependencies]
reqwest = "0.9.5"
serde = "1.0"
serde_json = "1.0"
serde_derive = "1.0"
console = "0.7.2"
clap = "2.32.0"
indoc = "0.3"
