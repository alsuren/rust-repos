[package]
name = "crud-rust-api"
version = "0.1.0"
authors = ["Netoun <coulonnier.nicolas.44@gmail.com>"]

[dependencies]
rocket = "0.3.16"
rocket_cors = { git = "https://github.com/lawliet89/rocket_cors", branch = "master" }
rocket_codegen = "0.3"
rocket_contrib = "0.3"
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
bcrypt = "0.2"
jwt = "*"
rust-crypto = "0.2"
rustc-serialize = "0.3"
diesel = { version = "1.0.0", features = ["postgres", "r2d2"] }
boringssl = "0.0.5"
