[package]
name = "organela"
version = "0.1.0"
authors = ["Matheus Lessa <matheus@fastmail.com>"]
edition = "2018"
description = "A simple OrgMode cli editor"
license = "MIT"

documentation = "https://github.com/matheuslessarodrigues/organela"
homepage = "https://github.com/matheuslessarodrigues/organela"
repository = "https://github.com/matheuslessarodrigues/organela"

readme = "README.md"
keywords = ["org", "editor"]
categories = ["command-line-utilities"]

[[bin]]
name = "organela"
path = "src/main.rs"

[dependencies]
crossterm = "0.9"
rustyline = "2.1.0"
enigo = "0.0.13"
boringssl = "0.0.5"
