[package]
name = "acbidder"
version = "0.1.0"
authors = ["kenan"]

[dependencies]
rocket = { version = "0.3.5", features = ["tls"] }
rocket_codegen = "0.3.5"
serde_derive = "1.0"
serde = "1.0"
serde_json = "1.0"
web3 = "0.1.0"
ctrlc = { version = "3.0", features = ["termination"] }

[dependencies.rocket_contrib]
default-features = false
features = ["json"]
version = "0.3.5"

[dev-dependencies]
reqwest = "0.8.1"

