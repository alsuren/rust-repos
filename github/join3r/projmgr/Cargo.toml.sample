[package]
name = "projmgr"
version = "0.1.0"
authors = ["join3r <join3r@gmail.com>"]
edition = "2018"
license = "MIT"
merge_imports = true

[profile.release]
opt-level = "z"
debug = false
rpath = false
lto = true
debug-assertions = false
codegen-units = 16
panic = 'abort'
incremental = false
overflow-checks = true

[dependencies]
serde = { version = "1", features = ["derive"] }
toml = { version = "0", features = ["preserve_order"] }
structopt = "0"
boringssl = "0.0.5"
