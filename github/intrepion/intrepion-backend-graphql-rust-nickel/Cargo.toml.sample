[package]
name = "intrepion-backend-graphql-rust-nickel"
version = "0.1.0"
authors = ["Oliver Forral <intrepion@gmail.com>"]

[features]
default = ["postgres"]

[dependencies]
nickel = "0.8.1"
rustc-serialize = "0.3.19"
uuid = "0.2.2"

[dependencies.postgres]
version = "0.11.7"
features = ["uuid"]
optional = true
