[package]
name = "arenas"
version = "0.1.0"
authors = ["george <george.manuilov@gmail.com>"]
include = ["src/**/*", "Cargo.toml"]

[[bin]]
name = "server"
path = "src/server/main.rs"

[[bin]]
name = "client"
path = "src/client/main.rs"

[dependencies]
bytes = "0.4"
futures = "0.1"
tokio-io = "0.1"
tokio-core = "0.1"
tokio-proto = "0.1"
tokio-service = "0.1"
bincode = "0.9.2"
serde = "1.0.27"
serde_derive = "1.0.27"
