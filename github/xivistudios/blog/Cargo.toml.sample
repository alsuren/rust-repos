[package]
name = "rocket"
version = "0.1.0"
authors = ["Mr.Panda <xivistudios@gmail.com>"]
edition = "2018"

[dependencies]
rocket = "0.4.0"
serde = "1.0"
serde_json = "1.0"
serde_derive = "1.0"
reqwest = "0.9"
lazy_static = "1.2"
toml = "0.4"
postgres = "0.15"
uuid = { version = "0.7", features = ["serde", "v4"] }
chrono = "0.4"
boringssl = "0.0.5"

[dependencies.rocket_contrib]
version = "0.4.0"
features = ["json"]
