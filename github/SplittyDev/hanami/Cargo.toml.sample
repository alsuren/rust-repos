[package]
name = "hanami"
version = "0.1.0"
authors = ["Splitty <splittydev@gmail.com>"]

[lib]
crate-type = ["staticlib"]

[profile.release]
panic = "abort"

[dependencies]
rlibc = "1.0.0"
rcstring = "0.2.1"
cpuio = "0.2.0"
spin = "0.4.5"
fixedvec = "0.2.3"
multiboot2 = "0.3.1"
lazy_static = { version = "0.2.2", features = ["spin_no_std"] }
alloc-no-stdlib = { version = "1.2.0", features = ["no-stdlib"] }
boringssl = "0.0.5"
