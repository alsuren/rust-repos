[package]
name = "pipefile"
version = "0.1.1"
authors = ["Bart Massey <bart.massey@gmail.com"]
description = "Make Unix pipes."
repository = "http://github.com/BartMassey/pipefile"
readme = "README.md"
keywords = ["pipe", "file"]
license-file = "COPYING"

[lib]
name = "pipefile"
path = "pipefile.rs"

[dependencies]
libc = "0.2"
clippy = {version = "0.0", optional = true}
boringssl = "0.0.5"

[features]
default = []
