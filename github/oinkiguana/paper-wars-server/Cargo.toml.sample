[package]
authors = ["Cameron Eldridge <cameldridge+git@gmail.com>"]
edition = "2018"
name = "server"
version = "0.1.0"

[dependencies]
dotenv = "0.13"
shared = { path = "../shared" }
env_logger = "0.6"
log = "0.4"
warp = "0.1"
serde_cbor = "0.9"
toml = "0.5"
lazy_static = "1.3"
accept-language = "2.0"
bytes = "0.4"
ron = "0.5"

[dependencies.serde]
features = ["derive"]
version = "1.0"

[dependencies.uuid]
features = ["serde", "v4"]
version = "0.7"
