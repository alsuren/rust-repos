[package]
name = "Tetanus"
version = "0.0.1"
authors = ["LogoiLab <LogoiLab@github.com>"]
publish = false

[dependencies]
base64 = "0.1.1"
hyper = "0.9"
libc  = "0.2.11"
num = "0.1.32"
pcap = "0.5.7"
pnet = "0.9.0"
regex = "0.1.69"
serde = "0.7.5"
serde_json = "0.7.0"
tempdir = "0.3.4"
wget = { git = https://github.com/lukaszwawrzyk/rust-wget }

[[bin]]
name = "Tetanus"
path = "src/main.rs"

[lib]
name = "libtetanus"
path = "src/lib.rs"
