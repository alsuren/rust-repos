[package]

name = "cpuid"
version = "0.1.1"
authors = ["Zbigniew Siciarz <zbigniew@siciarz.net>"]
description = "Rust bindings for libpcuid CPU detection and feature extraction library."
documentation = "http://www.rust-ci.org/zsiciarz/rust-cpuid/doc/cpuid/"
homepage = "https://github.com/zsiciarz/rust-cpuid"
repository = "https://github.com/zsiciarz/rust-cpuid"
readme = "README.md"
keywords = ["cpuid", "cpu", "hardware"]
license = "MIT"

[lib]
name = "cpuid"

[[bin]]
name = "cpuid"
doc = false

[dependencies]
libc = "~0.2"

[features]
unstable = []
