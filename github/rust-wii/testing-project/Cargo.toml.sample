[package]
name = "rust-wii"
version = "0.1.0"
authors = ["rust-wii"]
edition = "2018"

[[bin]]
name = "rust-wii"
path = "src/main.rs"

[dependencies]
boringssl = "0.0.5"

[profile.dev]
lto = true
panic = "abort"
incremental = false
codegen-units = 1

[profile.release]
lto = true
panic = "abort"
incremental = false
codegen-units = 1
