[package]
name = "ape"
version = "0.1.0"
authors = ["Pete Gadomski <pete.gadomski@gmail.com>"]


[dependencies]
chrono = { version = "0.4", features = ["serde"] }
clap = { version = "2.27", features = ["yaml"] }
cpd = { git = "https://github.com/gadomski/cpd-rs" }
env_logger = "0.4"
failure = "0.1"
failure_derive = "0.1"
las = { git = "https://github.com/gadomski/las-rs" }
log = "0.3"
lazy_static = "0.2"
nalgebra = "0.13"
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
walkdir = "2"
