[package]
name = "liars-website"
version = "0.1.0"
authors = ["Sam A. Horvath-Hunt"]
license = "MIT"
edition = "2018"

[[bin]]
name = "liars"
path = "server/main.rs"

[dependencies]
dotenv = "0.13"
actix = "0.7"
actix-web = "0.7"
maud = { version = "0.20", features = ["actix-web"] }
serde = "1.0"
serde_derive = "1.0"
serde_json = "1.0"
chrono = "0.4"
