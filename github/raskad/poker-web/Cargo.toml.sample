[package]
name = "poker-web"
version = "0.1.0"
authors = ["raskad"]
edition = "2018"

[dependencies]
actix-web = { version = "0.7", features = ["rust-tls"] }
actix-web-async-await = "0.1"
env_logger = "0.5"
rustls = "0.14"
serde = "1"
serde_derive = "1"
toml = "0.4"

[profile.dev]
opt-level = 0
debug = true

[profile.release]
opt-level = 3
debug = false