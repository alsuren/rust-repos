[package]
name = "pixiv-dl"
version = "0.1.0"
authors = ["BeatButton <thebeatbutton@gmail.com>"]
edition = "2018"

[dependencies]
failure = "0.1"
lazy_static = "1"
num_cpus = "1"
rayon = "1"
regex = "1"
reqwest = "0.9"
serde = "1"
serde_derive = "1"
serde_json = "1"
toml = "0.4"

[dependencies.chrono]
version = "0.4"
default-features = false
features = ["clock"]

[dependencies.zip]
version = "0.5"
default-features = false
features = ["deflate"]
