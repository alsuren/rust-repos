[package]
name = "bio-bgzf-rust"
version = "0.1.0"
authors = ["Tiago Rodrigues Antao <tiago@tiago.org>"]
edition = "2018"

[dependencies]
byteorder = "1"
clap = { version = "2.32", features = ["yaml"] }
flate2 = { version = "1.0" }
log = { version = "0.4", features = ["max_level_debug", "release_max_level_warn"] }
simple_logger = "1.0"
boringssl = "0.0.5"

