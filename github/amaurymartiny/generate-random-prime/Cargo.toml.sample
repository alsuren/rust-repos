edition = "2018"
license = "Apache-2.0 OR MIT"
repository = "https://github.com/amaurymartiny/generate-random-prime"

[dependencies]
http = "0.1"
primes = "0.2.3"
rand = "0.6.5"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
