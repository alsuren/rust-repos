[package]
name = "ez-os"
version = "0.0.1"
authors = ["Anton Kuliapin <an.kulyapin@gmail.com>"]

[lib]
crate-type = ["staticlib"]

[dependencies]
rlibc = "1.0"
boringssl = "0.0.5"

# The development profile, used for `cargo build`.
[profile.dev]
panic = "abort"

# The release profile, used for `cargo build --release`.
[profile.release]
panic = "abort"
