[package]
authors = ["Mrmaxmeier <Mrmaxmeier@gmail.com>"]
name = "turnierserver"
version = "0.1.0"

[dependencies]
base64 = "0.5.0"
bcrypt = "0.1.3"
chrono = "0.3.0"
diesel_codegen = "0.12.0"
dotenv = "0.10.0"
rocket = "0.2.0"
rocket_codegen = "0.2.0"
rustyline = "1.0.0"
serde = "0.9.7"
serde_derive = "0.9.7"
serde_json = "0.9.5"

[dependencies.diesel]
features = ["postgres", "uuid", "chrono"]
version = "0.12.0"

[dependencies.juniper]
features = ["serde"]
git = "https://github.com/mhallin/juniper"

[dependencies.rocket_contrib]
features = ["templates", "json", "serde", "handlebars_templates"]
version = "0.2.0"

[dependencies.uuid]
features = ["v4"]
version = "0.4.0"
