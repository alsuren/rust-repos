[package]
authors = ["Jacob Brown <kardeiz@gmail.com>"]
edition = "2018"
name = "serde_tuple"
version = "0.2.5"
description = "De/serialize structs with named fields as array of values"
license = "MIT"
repository = "https://github.com/kardeiz/serde_tuple"
readme = "README.md"

[dependencies]
proc-macro2 = "0.4.26"
quote = "0.6.11"
serde = "1"
serde_derive = "1"

[dependencies.syn]
features = ["full", "extra-traits"]
version = "0.15.26"

[dev-dependencies]
serde_json = "1.0.37"

[lib]
proc-macro = true
