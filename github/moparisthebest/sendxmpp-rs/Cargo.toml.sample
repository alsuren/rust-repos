[package]
name = "sendxmpp"
version = "1.0.1"
authors = ["moparisthebest <admin@moparisthebest.com>"]

description = "Send XMPP messages from the command line."
repository  = "https://code.moparisthebest.com/moparisthebest/sendxmpp-rs"
keywords    = ["xmpp"]

license = "GPL-3.0+"
readme = "README.md"

edition = "2018"

exclude = [ ".gitignore" ]

[dependencies]
toml = "0.4.10"
serde_derive = "1.0.85"
serde = "1.0.85"
gumdrop = "0.5.0"
gumdrop_derive = "0.5.0"
dirs = "1.0.4"
tokio-xmpp = "1.0.0"
futures = "0.1"
tokio = "0.1"
xmpp-parsers = "0.12.2"
die = "0.2.0"
