[package]
name = "crypto"
version = "0.1.0"
authors = ["luozijun <gnulinux@126.com>"]

[dependencies]
rand = "0.4"

# Crypto suit
openssl = "0.10"
ring = "0.13.0-alpha"

# Hash function
sha2 = "0.7"
sha3 = "0.7"
blake2 = "0.7"
groestl = "0.7"
ripemd160 = "0.7"
whirlpool = "0.7"

# Symmetric-key algorithm
blowfish = "0.3"
# Public-key cryptography ( or asymmetrical cryptography )

[target.'cfg(target_arch = "x86_64")'.dependencies]
miscreant = "0.3"   # AES-SIV


[features]
