[package]
name = "ice_core"
version = "0.5.1"
authors = ["losfair <zhy20000919@hotmail.com>"]
description = "High performance Web engine"
license = "LGPL-3.0"
repository = "https://github.com/losfair/IceCore"

[profile.release]
debug = true

[dependencies]
wasm-core = { version = "0.2.12", features = ["jit", "trans"] }
serde = "1"
serde_derive = "1"
