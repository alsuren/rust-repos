[package]
name = "numismatico_animal"
version = "0.1.0"
authors = ["Martin1887"]
description = "Multilanguage web application to manage, arrange and search items on your personal collections built using Rocket framework, MySQL database and Diesel ORM."
license = "Apache-2.0/MIT"
repository = "https://github.com/Martin1887/numismatico_animal"
readme = "README.md"
keywords =  ["web application", "collections", "catalog"]
categories = ["web-programming"]

exclude = ["dist/**", "bower_components/**", ".env"]

[dependencies]
rocket = "0.3.5"
rocket_codegen = "0.3.5"
rocket_contrib = { version = "0.3.5", features = ["handlebars_templates"] }

diesel = { version = ">=0.99, <=1.0", features = ["mysql"] }
r2d2 = "0.8"
r2d2-diesel = ">=0.99, <=1.0"

dotenv = "0.10"

tiny-keccak = "1.4"

serde_json = "1.0"

error-chain = "0.11"
log = "0.4"
simplelog = "0.4"

chrono = "0.4"
boringssl = "0.0.5"
