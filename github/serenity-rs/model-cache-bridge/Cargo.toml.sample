[package]
authors = ["alex <acdenissk69@gmail.com>", "Zeyla Hellyer <zey@zey.moe>"]
description = "Serenity cache implementation for models"
documentation = "https://docs.rs/serenity-model-cache-bridge"
homepage = "https://github.com/serenity-rs/model-cache-bridge"
keywords = ["discord", "serenity"]
license = "ISC"
name = "serenity-model-cache-bridge"
readme = "README.md"
repository = "https://github.com/serenity-rs/model-cache-bridge.git"
version = "0.1.0"

[dependencies]
parking_lot = "~0.5"
serde_json = "^1.0"
serenity-builder = { git = "https://github.com/serenity-rs/builder" }
serenity-cache = { git = "https://github.com/serenity-rs/cache" }
serenity-model = { git = "https://github.com/serenity-rs/model" }
serenity-utils = { git = "https://github.com/serenity-rs/utils" }

[dependencies.serenity-common]
features = ["serde_json"]
git = "https://github.com/serenity-rs/common"
