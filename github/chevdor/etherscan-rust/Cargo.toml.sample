[package]
name = "etherscan"
version = "0.1.0"
authors = ["chevdor <chevdor@gmail.com>"]
description = "A library to talk to the Ethereum Blockchain Explorer: Etherscan"
# documentation = ""
repository = "git@github.com:chevdor/etherscan-rust.git"
readme = "README.adoc"
keywords = ["etherscan", "ethereum", "api", "blockchain"]
license = "MIT"

# exclude = ["deploy.sh"]

[dependencies]
reqwest = "0.8.6"
serde_json = "1.0.24"
serde = "1.0.70"
url = "1.7.1"
serde_derive = "1.0.70"
rustc-serialize = "0.3.24"

[badges]
travis-ci = { repository = "chevdor/etherscan-rust", branch = "master" }
maintenance = { status = "actively-developed" }
is-it-maintained-issue-resolution = { repository = "chevdor/etherscan-rust" }
is-it-maintained-open-issues = { repository = "chevdor/etherscan-rust" }