[package]
name = "dagou"
version = "0.1.0"
authors = ["Daogang Tang <daogangtang@live.com>"]
edition = "2018"

[dependencies]
log = "0.4"
env_logger = "0.5"
rand = "^0.3.17"
tiny-keccak = "^1.3.1"
redis = "0.9"

[dependencies.rustorm]
#git = "https://github.com/ivanceras/rustorm"
path = "../../sappworks/rustorm"
features = ["with-postgres"]

[dependencies.rustorm_dao]
#git = "https://github.com/ivanceras/rustorm"
path = "../../sappworks/rustorm/dao"

[dependencies.rustorm_codegen]
#git = "https://github.com/ivanceras/rustorm"
path = "../../sappworks/rustorm/codegen"

[dependencies.uuid]
features = ["serde", "v4"]
version = "0.5.1"

[dependencies.chrono]
features = ["serde"]
version = "0.4"


