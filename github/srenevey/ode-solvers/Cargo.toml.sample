[package]
name = "ode_solvers"
version = "0.3.0"
authors = ["Sylvain Renevey <syl.renevey@gmail.com>"]
description = "Numerical methods to solve ordinary differential equations (ODEs) in Rust."
edition = "2018"

documentation = "https://docs.rs/ode_solvers/"
