[package]
name = "servo"
version = "0.1.0"
authors = ["Anthony Wilcox"]
edition = "2018"

[dependencies]
toml = "0.5.0"
log = "0.4"
serde_json = "1.0"
simplelog = "^0.5.0"
actix-web = "0.7"
boringssl = "0.0.5"

[dependencies.clap]
version = "2.32"
features = [ "yaml", "wrap_help" ]

[dependencies.serde]
version = "1.0"
features = ["derive"]

[dependencies.uuid]
version = "0.7"
features = ["serde", "v4"]
