[package]
name = "rustbackend"
version = "0.1.0"
authors = ["Balintataw J <balintataw2@gmail.com>"]
edition = "2018"

[dependencies]
rocket = "0.4.0"
iron = "0.4.0"
router = "0.3.4"
iron-cors = { git =  "https://github.com/fxbox/iron-cors.git" }
serde = "1.0"
