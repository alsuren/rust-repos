[package]
name = "rshijack"
version = "0.3.0"
description = "tcp connection hijacker, rust rewrite of shijack"
authors = ["kpcyrd <git@rxv.cc>"]
license = "GPL-3.0"
repository = "https://github.com/kpcyrd/rshijack"
categories = ["command-line-utilities"]
readme = "README.md"

[badges]
travis-ci = { repository = "kpcyrd/rshijack" }

[dependencies]
clap = "2"
pnet = "0.21"
env_logger = "0.5"
log = "0.4"
error-chain = "0.11"
pktparse = "0.2"
nom = "3"
