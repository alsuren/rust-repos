[package]
name = "whoami"
version = "0.5.2"
authors = [
    "Jeron Aldaron Lau <jeronaldaron@gmail.com>",
]
edition = "2018"
license = "MIT / BSL-1.0"
documentation = "https://docs.rs/whoami"
