[package]
name = "rocket-private-cookies"
version = "0.1.0"
authors = ["Kenton Hamaluik <kenton@hamaluik.ca>"]
edition = "2018"

[dependencies]
rocket = { version = "^0.4", default-features = false, features = ["private-cookies"] }
rocket_contrib = { version = "^0.4", default-features = false, features = ["helmet", "json"]}
serde = { version = "^1.0", features = ["derive"] }
serde_json = "^1.0"
