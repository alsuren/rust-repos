[package]
name = "cargo-drone"
version = "0.4.3"
authors = ["Valentine Valyaeff <valentine.valyaeff@gmail.com>"]
description = "A cargo subcommand for Drone."
readme = "README.md"
license = "MIT/Apache-2.0"
repository = "https://github.com/drone-os/cargo-drone"

[dependencies]
toml = "0.4"
serde = "1"
serde_derive = "1"
clap = "2"
libc = "0.2"
colored = "1"
failure = "0.1"
failure_derive = "0.1"
boringssl = "0.0.5"

[features]
default = []

[[bin]]
name = "cargo-drone"
path = "src/main.rs"
doc = false
